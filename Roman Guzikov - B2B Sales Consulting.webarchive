            }
            
            if (targetElement) {
                const headerHeight = document.querySelector('.header').offsetHeight || 80;
                const targetPosition = targetElement.offsetTop - headerHeight - 20;
                
                console.log(`Scrolling to ${targetId} at position ${targetPosition}`);
                
                window.scrollTo({
